{
    "tasks": {
        "prisma:migrate": {
            "outputs": [
                "node_modules/@prisma/client/**"
            ]
        },
        "dev": {
            "dependsOn": [
                "prisma:migrate"
            ],
            "persistent": true,
            "cache": false
        },
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        }
    }
}